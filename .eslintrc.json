    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react"
    ],
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "rules": {
      "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
    }
}